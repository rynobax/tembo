{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2015"],
    "jsx": "react",
    "strict": true,
    "noImplicitAny": true,
    "types": ["jest"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "baseUrl": "src/",
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "exclude": ["index.js", "android", "ios", "build", "node_modules", ".coverage", "native-assets", "jest"]
}