{
  "name": "tembo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "test": "jest"
  },
  "dependencies": {
    "expo": "^31.0.5",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.1.tar.gz"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/react": "^16.3.14",
    "@types/react-native": "^0.57.10",
    "@types/react-test-renderer": "^16.0.3",
    "expo-cli": "^2.4.0",
    "jest-expo": "~27.0.0",
    "react-native-scripts-ts": "1.15.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.6.3",
    "ts-jest": "^22.4.6",
    "tslib": "^1.9.1",
    "typescript": "^3.1.6"
  },
  "main": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
